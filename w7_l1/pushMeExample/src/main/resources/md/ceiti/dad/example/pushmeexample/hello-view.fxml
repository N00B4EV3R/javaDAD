<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="175.0" prefWidth="427.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="md.ceiti.dad.example.pushmeexample.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="textField" prefHeight="15.0" prefWidth="387.0">
      <font>
         <Font size="15.0" />
      </font></TextField>
    <Button onAction="#onHelloButtonClick" text="Type something in the box then push me">
      <font>
         <Font size="14.0" />
      </font></Button>

    <Label fx:id="welcomeText" />
</VBox>
